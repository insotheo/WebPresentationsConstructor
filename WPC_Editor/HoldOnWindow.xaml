<Window x:Class="WPC_Editor.HoldOnWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPC_Editor"
        mc:Ignorable="d"
        Title="WebPresentationsConstructor -> procname..."
        Height="125" Width="400"
        SizeToContent="Width"
        ShowInTaskbar="False"
        MouseDown="Window_MouseDown"
        Topmost="True"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        Background="{StaticResource windowBackColor}"
        BorderBrush="{StaticResource backSelectedItem}"
        BorderThickness="3">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="titleTB" Foreground="White" FontSize="24" VerticalAlignment="Center" FontWeight="SemiBold" FontFamily="Roboto"/>
        <ProgressBar Grid.Row="1" Foreground="LightBlue" IsIndeterminate="True" Margin="0 10"/>
    </Grid>
</Window>
